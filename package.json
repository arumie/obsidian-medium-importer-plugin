{
    "name": "obsidian-medium-importer-plugin",
    "version": "0.1.0",
    "description": "This is a plugin for Obsidian that allows you to import Medium posts into your vault.",
    "main": "main.js",
    "scripts": {
        "dev": "node esbuild.config.mjs",
        "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
        "build:css": "postcss ./src/global.css -o styles.css",
        "dev:css": "postcss ./src/global.css -o styles.css --watch",
        "release": "PACKAGE_VERSION=$(cat package.json | grep \\\"version\\\" | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]') && git tag $PACKAGE_VERSION",
        "version": "node version-bump.mjs && git add manifest.json versions.json && release"
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^20.12.12",
        "@types/tough-cookie": "^4.0.5",
        "@typescript-eslint/eslint-plugin": "5.29.0",
        "@typescript-eslint/parser": "5.29.0",
        "builtin-modules": "3.3.0",
        "esbuild": "0.21.4",
        "obsidian": "latest",
        "tslib": "2.4.0",
        "typescript": "4.7.4"
    },
    "dependencies": {
        "autoprefixer": "^10.4.14",
        "fs": "^0.0.1-security",
        "postcss-cli": "^10.1.0",
        "postcss-import": "^15.1.0"
    }
}
